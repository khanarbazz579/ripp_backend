{
  "name": "Ripple-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "start-dev": "NODE_ENV_HEADER=local nodemon --inspect ./bin/www",
    "test": "NODE_ENV=test mocha --timeout 600000 ./test/**/**.test.js --exit",
    "test-dev": "NODE_ENV=test mocha --timeout 600000 ./test/**/**.test.js --exit",
    "coverage": "NODE_ENV=test nyc --reporter=html mocha --timeout 600000 ./test/**/**.test.js --exit",
    "db-migrations": "node_modules/.bin/sequelize db:migrate --config=config/config.json",
    "test-migrations": "node_modules/.bin/sequelize db:migrate --config=config/config.json --env test",
    "seed": "node_modules/.bin/sequelize db:seed:all --config=config/config.json",
    "seedOne": "node_modules/.bin/sequelize db:seed --seed --config=config/config.json"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^1.6.0",
    "@types/archiver": "^3.0.0",
    "archiver": "^3.1.1",
    "async": "^2.6.3",
    "aws-sdk": "2.368.0",
    "bcrypt": "^2.0.1",
    "bcrypt-promise": "^2.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cron": "^1.7.2",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^5.0.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "fast-csv": "^2.5.0",
    "gm": "^1.23.1",
    "googleapis": "^27.0.0",
    "html-to-text": "^4.0.0",
    "ical-generator": "^1.8.1",
    "imap": "^0.8.19",
    "jade": "~1.11.0",
    "js-base64": "^2.5.1",
    "jsdom": "^15.1.1",
    "mimemessage": "^1.0.5",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql2": "^1.7.0",
    "nodemailer": "^4.7.0",
    "nodemon": "^1.19.2",
    "parse-error": "^0.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "raven": "^2.6.4",
    "request": "^2.88.0",
    "s3-zip": "^2.0.1",
    "sequelize": "^4.44.3",
    "sequelize-cli": "^5.5.1",
    "sequelize-datatable": "^2.0.0",
    "serve-favicon": "~2.4.5",
    "simple-oauth2": "^2.4.0",
    "socket.io": "^2.1.1",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2"
  }
}
